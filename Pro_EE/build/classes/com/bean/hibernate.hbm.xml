<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.bean.Employed" table="scott.emp" catalog="orcl">
		<id name="empno" type="java.lang.Integer">
			<column name="empno"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="ename"></column>
		</property>
		<property name="job" type="java.lang.String">
			<column name="job"></column>
		</property>
		<property name="deptno" type="java.lang.Integer">
			<column name="deptno"></column>
		</property>

	</class>
</hibernate-mapping>

  